# Disable optional features for testing
ENABLE_INTELLIGENCE_ENGINE=false
ENABLE_PREDICTIVE_METRICS=false
ENABLE_REVENUE_METRICS=false
ENABLE_WELLBEING_METRICS=false
ENABLE_FAN_METRICS=false
