# FlexTime Backend Configuration - MOCK VERSION FOR DEVELOPMENT

# Server settings
PORT=3004
NODE_ENV=development
LOG_LEVEL=debug

# Database configurations - MOCK VALUES
NEON_DB_CONNECTION_STRING=postgresql://demo:password@demo.neon.tech/HELiiX
NEON_DB_HOST=demo.neon.tech
NEON_DB_PORT=5432
NEON_DB_NAME=HELiiX
NEON_DB_USER=demo
NEON_DB_PASSWORD=password
NEON_DB_SSL=false
ENABLE_NEON_MEMORY=true
USE_NEON_DB=true

# In-container database
